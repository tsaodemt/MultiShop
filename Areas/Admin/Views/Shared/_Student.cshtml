<table class="table table-bordered">
    <tr>
        <th>Họ tên</th>
        <th>Ngày tháng năm sinh</th>
        <th>Note</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (Student c in ViewBag.Students)
    {
        <tr>
            <td>@c.FullName</td>
            <td>@c.NoteDate</td>
            <td>@Html.Raw(c.Note)</td>
            <td>
                <a href="/Admin/Home/Delete/@c.StudentID">
                    Delete
                </a>
            </td>
            <td>
                <a href="/Admin/Home/Edit/@c.StudentID">
                    Edit
                </a>
            </td>
        </tr>
    }
</table>